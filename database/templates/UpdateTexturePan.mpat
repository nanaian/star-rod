
#new:Script $AUTO_Script_UpdateTexturePan
{
SetGroup  00000000
If  *Var[5]  ==  00000001
	If  *Var[6]  ==  00000001
		If  *Var[7]  ==  00000001
			If  *Var[8]  ==  00000001
				Call  $AUTO_Function_UpdatePanSimple ( )
				Return
			EndIf
		EndIf
	EndIf
EndIf
Call  $AUTO_Function_UpdatePanComplex ( )
Return
End
}

